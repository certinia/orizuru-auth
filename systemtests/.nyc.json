{
	"all": true,
	"branches": 80,
	"cache": true,
	"check-coverage": true,
	"exclude": [
		"**/*.d.ts"
	],
	"extension": [
		".ts"
	],
	"functions": 80,
	"include": [
		"src",
		"systemtests"
	],
	"instrument": true,
	"lines": 80,
	"per-file": false,
	"report-dir": "coverage-systemtests",
	"reporter": [
		"lcov",
		"text",
		"text-summary"
	],
	"require": [
		"ts-node/register"
	],
	"statements": 80
}
