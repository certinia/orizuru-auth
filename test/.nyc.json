{
	"all": true,
	"branches": 100,
	"check-coverage": true,
	"exclude": [
		"coverage*",
		"dist",
		"doc",
		"systemtests",
		"**/*.d.ts"
	],
	"extension": [
		".ts"
	],
	"functions": 100,
	"instrument": true,
	"lines": 100,
	"per-file": true,
	"reporter": [
		"lcov",
		"text",
		"text-summary"
	],
	"require": [
		"ts-node/register"
	],
	"sourceMap": true,
	"statements": 100
}
