{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"completed-docs": [
			true,
			"enums",
			"functions",
			"methods"
		],
		"indent": [
			true,
			"tabs",
			4
		],
		"interface-name": false,
		"max-line-length": false,
		"no-any": true,
		"no-namespace": [
			false,
			"allow-declarations"
		],
		"no-trailing-whitespace": true,
		"no-unused-expression": false,
		"quotemark": [
			true,
			"single"
		],
		"trailing-comma": [
			true,
			{
				"multiline": "never",
				"singleline": "never"
			}
		]
	},
	"rulesDirectory": []
}
